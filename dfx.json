{
  "canisters": {
    "procurement": {
      "type": "motoko",
      "main": "canisters/procurement/src/main.mo",
      "declarations": {
        "output": "frontend/src/declarations/procurement",
        "bindings": ["ts"],
        "env_override": null
      }
    },
    "helix_frontend": {
      "type": "assets",
      "source": ["frontend/dist"],
      "dependencies": ["procurement"]
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "type": "ephemeral"
    },
    "ic": {
      "providers": ["https://ic0.app"],
      "type": "persistent"
    }
  },
  "version": 1
}